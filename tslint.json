{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "interface-over-type-literal": false,
      "max-classes-per-file": false,
      "no-empty": false,
      "max-line-length": [true, 225],
      "curly": [true, "ignore-same-line"],
      "ordered-imports": false
    },
    "rulesDirectory": []
}
